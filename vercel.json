{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": { 
          "distDir": "dist/angular-boilerplate",
          "buildCommand": "npm run vercel-build"
        }
      }
    ],
    "routes": [
      {
        "src": "/(assets/.+)",
        "dest": "/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }